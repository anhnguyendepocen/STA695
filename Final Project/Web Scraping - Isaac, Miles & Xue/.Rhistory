install.packages("XML")
library(XML)
URL0=paste("http://www.spotrac.com/nfl/rankings/ ")
d0=htmlParse(URL0,encoding="UTF-8")
d0
d0.table<- readHTMLTable(d0,stringsAsFactors=F)
d0.data=data.frame(d0.table[[1]])
View(`d0.data`)
